{
  "name": "belafrica",
  "version": "1.0.0",
  "description": "BELAFRICA - Plateforme de communication pour la diaspora africaine en Europe",
  "author": "Rollin Loic Tianga",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:production": "ng build --configuration=production",
    "build:development": "ng build --configuration=development",
    "build:vercel": "ng build --configuration=vercel",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/belafrica/stats.json",
    "vercel-build": "npm run build:vercel",
    "vercel-dev": "ng serve --host 0.0.0.0 --port 3000",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:staging": "vercel --env staging",
    "serve:ssr": "node dist/belafrica/server/server.mjs",
    "build:ssr": "ng build && ng run belafrica:server:production",
    "prerender": "ng run belafrica:prerender"
  },
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-browser-dynamic": "^21.1.2",
    "@angular/router": "^21.1.2",
    "@angular/service-worker": "^21.1.2",
    "@angular/ssr": "^21.1.2",
    "@emailjs/browser": "^4.3.3",
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.5.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.5.4",
    "tslib": "^2.6.0",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.2",
    "@angular-eslint/builder": "21.2.0",
    "@angular-eslint/eslint-plugin": "21.2.0",
    "@angular-eslint/eslint-plugin-template": "21.2.0",
    "@angular-eslint/schematics": "21.2.0",
    "@angular-eslint/template-parser": "21.2.0",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~5.1.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^20.8.0",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@typescript-eslint/types": "^8.54.0",
    "@typescript-eslint/utils": "^8.54.0",
    "@vercel/analytics": "^1.0.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.1.0",
    "typescript": "~5.9.3",
    "vercel": "^34.0.0",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.7",
    "pnpm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "not IE 11",
      "last 2 versions",
      "Firefox ESR",
      "not UCAndroid >0",
      "not and_uc >0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
